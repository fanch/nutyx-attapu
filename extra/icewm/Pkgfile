# Depends on: gtk


name=icewm
version=1.3.4pre1
release=1
source=(http://downloads.sourceforge.net/$name/$name-$version.tar.gz)

build()
{

cd $name-$version
./configure --prefix=/usr
make
make DESTDIR=$PKG install

}

