# Depends on: xorg7


name=icewm
version=1.3.4pre1
release=1
source=(http://dfn.dl.sourceforge.net/sourceforge/icewm/icewm-1.3.4pre1.tar.gz)

build()
{

cd $name-$version
./configure --prefix=/usr
make
make DESTDIR=$PKG install

}

