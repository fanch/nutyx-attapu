# Depends on: flac, libsndfile

name=jack-audio-connection-kit
version=0.118.0
release=1
source=(http://jackaudio.org/downloads/$name-$version.tar.gz)

build() {
	cd $name-$version
	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
}
