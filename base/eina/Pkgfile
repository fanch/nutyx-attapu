name=eina
version=0.9.9.49539
release=1
source=(http://download.enlightenment.org/snapshots/LATEST/$name-$version.tar.bz2)

build()
{
	cd $name-$version 
	./configure --prefix=/usr
	make
	make install DESTDIR=$PKG
}

